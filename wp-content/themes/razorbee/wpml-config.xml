    </custom-fields>
    <admin-texts>
        <key name="nominee_theme_option">
            <key name="header-contact"/>
            <key name="text-logo"/>
            <key name="blog-title"/>
            <key name="donate-purpose"/>
            <key name="donate-modal-title"/>
            <key name="submit-button-text"/>
