<wpml-config>
    <custom-types>
        <custom-type translate="1">wpcf7_contact_form</custom-type>
        <custom-type translate="1">tt-reformation</custom-type>
        <custom-type translate="1">tt-issue</custom-type>
        <custom-type translate="1">tt-event</custom-type>
        <custom-type translate="1">tt-story</custom-type>
        <custom-type translate="1">tt-member</custom-type>
